{
  "name": "pipedrive-bling-integration-api",
  "version": "1.0.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "serve:dev": "ts-node-dev --inspect --ignore-watch node_modules --transpile-only src/infra/server.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jestUnit.config.js",
    "test:unit:verbose": "jest --passWithNoTests --noStackTrace --runInBand --watch -c jestUnit.config.js",
    "test:coverage": "npm test -- --coverage",
    "test:file": "jest --runTestsByPath"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.4",
    "ngrok": "^3.3.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/mongoose": "^5.10.2",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}
